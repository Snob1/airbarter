<%= simple_form_for(@transaction) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :wanted_product %>
    <%= f.input :wanted_product_quantity, collection: (1..10) %>
    <%= f.association :proposed_product %>
    <%= f.input :proposed_product_quantity, collection: (1..10) %>
    <%= f.input :address %>
    <%= f.input :photos, as: :attachinary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
