<%= simple_form_for(@entry, remote: remote) do |f| %>
  <%= f.error_notification %>


  <div class="form-inputs">
    <%= f.association :product %>
    <%= f.input :quantity, collection: (1..10) %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>

<% end %>
